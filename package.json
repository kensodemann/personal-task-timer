{
  "name": "personal-task-timer",
  "description": "A Personal Task Timer",
  "version": "2.2.0",
  "author": "Ken W. Sodemann",
  "homepage": "http://kensodemann.github.io/",
  "scripts": {
    "bump": "node scripts/bump-version.js",
    "build": "ng build",
    "build:web": "ionic build --engine=browser --prod -- --stats-json",
    "bundle-report": "webpack-bundle-analyzer www/stats.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "release": "node scripts/release.js",
    "start": "ng serve",
    "test": "jest --coverage",
    "test:ci": "jest --runInBand",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/fire": "~6.0.2",
    "@angular/forms": "~10.1.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@angular/service-worker": "~10.1.0",
    "@ionic/angular": "~5.3.2",
    "@ngrx/effects": "~10.0.0",
    "@ngrx/entity": "~10.0.0",
    "@ngrx/store": "~10.0.0",
    "core-js": "~3.6.5",
    "date-fns": "~2.16.1",
    "firebase": "~7.19.1",
    "rxjs": "~6.6.3",
    "tslib": "~2.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1001.0",
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular-devkit/core": "~10.1.0",
    "@angular-devkit/schematics": "~10.1.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@angular/language-service": "~10.1.0",
    "@ionic/angular-toolkit": "~2.3.3",
    "@ionic/prettier-config": "~1.0.0",
    "@types/jest": "~26.0.13",
    "@types/node": "~14.6.4",
    "adj-noun": "~0.1.1",
    "chalk": "~4.1.0",
    "codelyzer": "^6.0.0",
    "conventional-changelog": "~3.1.23",
    "conventional-changelog-cli": "~2.1.0",
    "execa": "~4.0.3",
    "husky": "~4.3.0",
    "inquirer": "~7.3.3",
    "jest": "~26.4.2",
    "jest-preset-angular": "~8.3.1",
    "listr": "~0.14.3",
    "prettier": "~2.1.1",
    "pretty-quick": "~3.0.0",
    "protractor": "~7.0.0",
    "rimraf": "~3.0.2",
    "semver": "~7.3.2",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "~3.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
